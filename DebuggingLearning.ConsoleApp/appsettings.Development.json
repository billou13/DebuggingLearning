{
    "NLog": {
        "rules": [
            {
                "logger": "System.*",
                "minLevel": "Warn",
                "final": true
            },
            {
                "logger": "Microsoft.*",
                "minLevel": "Warn",
                "final": true
            },
            {
                "logger": "*",
                "minLevel": "Trace",
                "writeTo": "console,file"
            }
        ]
    }
}