{
    "NLog": {
        "rules": [
            {
                "logger": "System.*",
                "finalMinLevel": "Warn"
            },
            {
                "logger": "Microsoft.*",
                "finalMinLevel": "Info"
            },
            {
                "logger": "*",
                "minLevel": "Trace",
                "writeTo": "console,file"
            }
        ]
    }
}